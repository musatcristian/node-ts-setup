{
  "ext": "ts json",
  "delay": 3,
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    "tests/**/*",
    "src/**/*.js"
  ],
  "restartable": "rs",
  "verbose": true,
  "execMap": {
    "ts": "ts-node -r dotenv/config"
  },
  "watch": [
    "src/**/*.ts"
  ],
  "env": {
    "NODE_ENV": "development"
  }
}